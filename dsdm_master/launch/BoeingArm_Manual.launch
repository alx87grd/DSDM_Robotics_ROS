<launch>

	<rosparam command="load" file="$(find dsdm_master)/config/BoeingArm.yaml"/>

        
	<node pkg="joy" type="joy_node" name="joy" output="screen">
        </node>

	<node pkg="dsdm_control" type="joystick_3DoF_openloop.py" name="R_ctl" output="screen">
